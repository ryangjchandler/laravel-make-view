{
    "name": "ryangjchandler/laravel-make-view",
    "description": "A simple `make:view` command for Laravel applications.",
    "keywords": [
        "ryangjchandler",
        "laravel",
        "laravel-make-view"
    ],
    "homepage": "https://github.com/ryangjchandler/laravel-make-view",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Ryangjchandler\\LaravelMakeView\\": "src",
            "Ryangjchandler\\LaravelMakeView\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ryangjchandler\\LaravelMakeView\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ryangjchandler\\LaravelMakeView\\LaravelMakeViewServiceProvider"
            ],
            "aliases": {
                "LaravelMakeView": "Ryangjchandler\\LaravelMakeView\\LaravelMakeViewFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
